$(".add_credit_user").on("submit", function() {

	if($('#produit_servicebundle_recrutement_username').val().length < minchamp1 || $('#produit_servicebundle_recrutement_username').val().length > maxchamp1){
		$("div.champ1").addClass("has-error");
		$("div.alertnotif1").show("slow").delay(6000).hide("slow");
		$('.control-info-champ1').html($('#produit_servicebundle_recrutement_username').val().length+'/'+maxchamp1);
		$('#produit_servicebundle_recrutement_username').focus();
		return false;
	}

	if($('#produit_servicebundle_recrutement_tel').val().length != 0 && !{{ number }}.test($('#produit_servicebundle_recrutement_tel').val())){
		$("div.champ2").addClass("has-error");
		$("div.alertnotif2").show("slow").delay(6000).hide("slow");
		$('.control-info-champ2').html('Not Number');
		$('#produit_servicebundle_recrutement_tel').focus();
		return false;
	}

	if($('#produit_servicebundle_recrutement_mail').val().length != 0 && !{{ mailregex }}.test($('#produit_servicebundle_recrutement_mail').val())){
		$("div.champ3").addClass("has-error");
		$("div.alertnotif3").show("slow").delay(6000).hide("slow");
		$('.control-info-champ3').html('Not Email');
		$('#produit_servicebundle_recrutement_mail').focus();
		return false;
	}
	if($('#pays_user').val() == 0){
		$("div.champ4").addClass("has-error");
		$("div.alertnotif4").show("slow").delay(6000).hide("slow");
		$('.control-info-champ4').html('');
		$('#pays_user').focus();
		return false;
	}

});

