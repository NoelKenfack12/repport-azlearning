body {
  margin: 0px;
  padding: 0px;
  font-family: "Open Sans", arial;
  color: #333;
}

[role="main"]:before {
  content: " ";
  position: absolute;
  z-index: -1;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
}

[role="main"] {
  width: 100%;
}

.sidebar-nav-learning {
  background-color: lightskyblue;
  width: 100%;
  max-width: 235px;
  margin: 0 auto;
  padding: 0;
  height: 100%;
  float: left;
  padding-top: 250px;
  padding-bottom: 250px;
}

[role="off-canvas"], [role="main"] {
  display: block;
  position: relative;
  z-index: 1;
  -webkit-transition: 0.08s all;
  -moz-transition: 0.08s all;
  -o-transition: 0.8s all;
  transition: 0.08s all;
}

[role="off-canvas"] {
  margin-left: -85%;
  width: 85%;
  float: left;
  z-index: 2;
}
@media all and (min-width: 550px) {
  [role="off-canvas"] {
    margin-left: -45%;
    width: 45%;
  }
}
@media all and (min-width: 1000px) {
  [role="off-canvas"] {
    margin-left: -25%;
    width: 25%;
  }
}
@media all and (min-width: 1260px) {
  [role="off-canvas"] {
    margin-left: -15%;
    width: 15%;
  }
}

[role="main"] {
  margin-left: 0;
  float: right;
  z-index: 1;
  position: relative;
}

.main-container, [role="navigation"], .footer-container {
  width: 100%;
  max-width: 1260px;
  margin: 0 auto;
}
[role="navigation"], .footer-container {
    overflow: hidden;
}

.header-learning, [role="navigation"], .footer-container {
  background-color: lightgray;
}

.logo {
  padding-left: 0 !important;
}

.header-learning {
  overflow: visible;
}
.header-learning i {
  line-height: 45px;
  cursor: pointer;
  width: 100px;
  position: relative;
  float: left;
  font-size: 1.9em;
  top: 0px;
  left: 15px;
}
.header-learning h1 {
  float: left;
}
.header-learning h1 a {
  text-decoration: none;
