<div style="min-height: 450px;">

<form class="col-md-12"  action="{{ path('users_adminuser_update_catalogue_chantier_invest', {'id': categorie.id}) }}" method="post" {{ form_enctype(form) }}>
	
	<div class="form-group">
		{{ form_widget(form.nom, {'attr': {'class': 'form-control', 'style': 'width: 100%;'}}) }}
		{{ form_label(form.nom, 'Nom du catalogue'|trans) }}
		<div style="color: red;">{{ form_errors(form.nom)|trans }}</div>
	</div>
	
	<div class="form-group">
		{{ form_widget(form.description, {'attr': {'class': 'form-control', 'style': 'width: 100%;'}}) }}
		{{ form_label(form.description, 'Description du la cat√©gorie'|trans) }}
		<div style="color: red;">{{ form_errors(form.nom)|trans }}</div>
	</div>
	
	<div class="form-group">
		{{ form_widget(form.rang, {'attr': {'class': 'form-control', 'style': 'width: 100%;'}}) }}
		{{ form_label(form.rang, 'Rang dans le classement'|trans) }}
		<div style="color: red;">{{ form_errors(form.nom)|trans }}</div>
	</div>
	
	<div class="form-group">
		{{ form_widget(form.file) }}
	</div>
	
	<div class="form-group">
	   <button class="btn btn-primary" type="submit">{{ 'Envoyer'|trans }} </button>
	</div>
	
{{ form_rest(form) }}
</form>

</div>
<script type="text/javascript" src="{{ asset('template/js/materialize.js') }}"></script>