    body {
	/* Colors */
		--color-blue-50: #0a84ff;
		--color-blue-60: #0060df;
		--color-blue-70: #003eaa;
		--color-blue-80: #002275;
		--color-gray-10: #f9f9fa;
		--color-gray-20: #ededf0;
		--color-gray-30: #d7d7db;
		--color-gray-40: #b1b1b3;
		--color-gray-50: #737373;
		--color-gray-60: #4a4a4f;
		--color-gray-70: #38383d;
		--color-gray-80: #2a2a2e;
		--color-gray-90: #0c0c0d;
		--color-green-70: #058b00;
		--color-teal-50: #00feff;
		--color-teal-60: #00c8d7;
		--color-teal-70: #008ea4;

	/* Icons */
		--icon-context-menu: url("chrome://global/skin/icons/more.svg");
		--icon-dismiss: url("chrome://global/skin/icons/close.svg");

	--background-color: #F9F9FA;
	--border-primary-color: #B1B1B3;

	/* Primary Action */
	--primary-action-default: var(--color-blue-60);
	--primary-action-hover: var(--color-blue-70);
	--primary-action-active: var(--color-blue-80);
	--primary-action-text-color: #fff;

	/* Secondary Action */
	--secondary-action-default: var(--color-gray-20);
	--secondary-action-hover: var(--color-gray-30);
	--secondary-action-active: var(--color-gray-40);
	--secondary-action-text-color: var(--color-gray-80);

	/* Ghost Action */
		--ghost-action-hover: var(--color-gray-20);
		--ghost-action-active: var(--color-gray-30);
		--ghost-action-text-color: var(--color-gray-80);

	/* Forms */
		--label-text-color: var(--text-secondary);
		--text-field-border-color: var(--color-gray-30);
		--text-field-hover-border-color: var(--color-gray-40);
		--text-field-focus-border-color: var(--color-gray-40);
		--text-field-bg-color: #fff;
	/* Focus */
	--focus-box-shadow: 0 0 0 2px #fff, 0 0 0 5px rgba(10, 132, 255, 0.5);

	/* Cards */
	--rasied-card-bg-color: #FFF;
	--rasied-card-border-radius: 8px;
	--box-shadow-10: 0px 0px 8px 0 rgba(12, 12, 13, 0.05);
	--raised-card-hover: 0 0 0 4px var(--secondary-action-default);
		
	/* Typography */
	--base-font-size: 15px;
	--small-font-size: 13px;
	--base-line-height: 24px;
	
	/* Text Colors */
	--text-primary: var(--color-gray-90);
	--text-secondary: var(--color-gray-50);
	}


	/* Primary Button */
	.btn--primary {
		background-color: var(--primary-action-default);
		color: var(--primary-action-text-color);
		fill: var(--primary-action-text-color);
		-moz-context-properties: fill;
	}

	.btn--primary:hover {
		background-color: var(--primary-action-hover);
	}

	.btn--primary:active {
		background-color: var(--primary-action-active);
	}

	.btn--primary:disabled,
	.btn--primary.disabled {
		background-color: var(--primary-action-default);
		color: var(--primary-action-text-color);
		fill: var(--primary-action-text-color);
		-moz-context-properties: fill;
	}

	/* Secondary Button */
	.btn--secondary {
		background-color: var(--secondary-action-default);
		color: var(--secondary-action-text-color);
		fill: var(--secondary-action-text-color);
		-moz-context-properties: fill;
	}

	.btn--secondary:hover {
		background-color: var(--secondary-action-hover);
	}

	.btn--secondary:active {
		background-color: var(--secondary-action-active);
	}

	.btn--secondary:disabled,
	.btn--secondary.disabled {
		background-color: var(--secondary-action-default);
		color: var(--secondary-action-text-color);
		fill: var(--secondary-action-text-color);
		-moz-context-properties: fill;
	}

	/* Pill Button */
	.btn--pill {
		border-radius: 16px;
		line-height: 32px;
		min-height: 32px;
		padding: 0 16px;
		vertical-align: middle;
	}

	.btn--pill-icon-only {
		background-position: 50%;
		background-repeat: no-repeat;
		background-size: 16px;
		width: 32px;
	}

	/* Form Button */
	.btn--form {
		border-radius: 4px;
		min-height: 40px;
		min-width: 96px;
		padding: 0 8px;
	}

	/* Ghost Button */
	.btn--ghost {
		background-color: transparent;
		color: var(--ghost-action-text-color);
		fill: var(--ghost-action-text-color);
		-moz-context-properties: fill;
	}

	.btn--ghost:hover {
		background-color: var(--ghost-action-hover);
	}

	.btn--ghost:active {
		background-color: var(--ghost-action-active);
	}

	.btn--ghost:disabled,
	.btn--ghost.disabled {
		background-color: transparent;
		color: var(--ghost-action-text-color);
		fill: var(--ghost-action-text-color);
		-moz-context-properties: fill;
	}

	.section-header {
		display: flex;
		margin-bottom: 12px;
	}

	.section-header > div {
		flex-grow: 1;
	}

	.title-card {
