	.card-app .context-menu-btn {
		opacity: 0;
		position: absolute;
		right: -16px;
		top: -16px;
		transform: scale(0.25);
		transition-duration: 150ms;
		transition-property: transform, opacity;	
	}

	/* Cards */
	.card-raised {
		background: var(--rasied-card-bg-color);
		border-radius: var(--rasied-card-border-radius);
